<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.Controller">
   <columnConstraints>
      <ColumnConstraints minWidth="10.0" prefWidth="100.0" />
      <ColumnConstraints />
      <ColumnConstraints minWidth="10.0" prefWidth="100.0" />
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Canvas fx:id="mainCanvas" height="800.0" width="900.0" GridPane.columnIndex="3" GridPane.rowIndex="1" />
      <GridPane GridPane.rowIndex="1">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="buttonStart" mnemonicParsing="false" onAction="#buttonStartHandler" prefHeight="96.0" prefWidth="96.0" stylesheets="@css/StartButtonCss.css" textAlignment="CENTER" />
            <Button fx:id="buttonOpen" mnemonicParsing="false" onAction="#buttonOpenHandler" prefHeight="96.0" prefWidth="96.0" stylesheets="@css/OpenButtonCss.css" textAlignment="CENTER" GridPane.rowIndex="1" />
         </children>
      </GridPane>
   </children>
</GridPane>
